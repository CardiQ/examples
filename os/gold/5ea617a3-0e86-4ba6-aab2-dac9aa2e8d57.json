    "minimal": [
      "`DOUBLE_CLICK` the Activities overview",
      "`CLICK` search bar",
      "`TYPING` 'Files'",
      "`CLICK` 'Files' to open the file manager",
      "`CLICK` 'Trash' in the sidebar",
      "`CLICK` the last deleted file",
      "`CLICK` 'Restore From Trash'"
